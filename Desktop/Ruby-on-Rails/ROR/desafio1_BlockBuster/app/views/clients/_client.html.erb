<div id="<%= dom_id client %>">
  <tr class="text-primary">
    <th scope="row"><%= client.id %></th>
    <td><%= client.name %></td>
    <td><%= client.age %></td>
    <td><%= client.movie.name %></td>
    <td><%= link_to "Mostrar cliente", client, class: "btn btn-secondary" %></td>
    <td><%= link_to "Editar cliente", edit_client_path(client), class: "btn btn-primary" %></td>
    <td><%= button_to "Eliminar cliente", client, method: :delete, class: "btn btn-warning" %></td>
  </tr>
</div>
